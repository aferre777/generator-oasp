{
  "name": "generator-oasp",
  "version": "1.1.2",
  "description": "Yeoman generator for AngularJS applications based on OASP4JS",
  "license": "Apache License 2.0",
  "main": "app/index.js",
  "repository": "oasp/generator-oasp",
  "author": "The OASP Team",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha -t 300000",
    "test-no-generate": "env no-generate=true mocha -t 0",
    "test-log-gulp": "env log-gulp=true mocha -t 0",
    "test-no-generate-log-gulp": "env log-gulp=true, no-generate=true mocha -t 0"
  },
  "files": [
    "app",
    "module",
    "controller",
    "directive",
    "utils.js"
  ],
  "keywords": [
    "yeoman-generator",
    "angular",
    "template"
  ],
  "dependencies": {
    "chalk": "^0.5.0",
    "ng-parse-module": "^0.1.0",
    "string": "^3.0.0",
    "underscore": "^1.7.0",
    "underscore.string": "^3.0.3",
    "yeoman-generator": "^0.18.0",
    "yosay": "^0.3.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "chai": "^2.1.0",
    "cross-spawn": "^0.2.6",
    "fs-extra": "^0.16.4",
    "glob": "^5.0.1",
    "mocha": "*",
    "rimraf": "^2.2.8"
  },
  "peerDependencies": {
    "yo": ">=1.0.0"
  }
}
